{
  "$schema": "../../zotero-def.json",
  "name": "zotero-obsidian-note",
  "version": "1.0.1",
  "description": "Zotero plugin to integrate with Obsidian.md",
  "scripts": {
    "lint": "eslint . --ext .ts --cache --cache-location .eslintcache/",
    "build": "tsc --noEmit && zt-build",
    "dev": "zt-dev",
    "release": "release-it"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/aidenlx/obsidian-zotero.git"
  },
  "author": "aidenlx",
  "bugs": {
    "url": "https://github.com/aidenlx/obsidian-zotero/issues"
  },
  "homepage": "https://github.com/aidenlx/obsidian-zotero",
  "zotero": {
    "name": "Obsidian Note for Zotero",
    "id": "zotero-obsidian-note@aidenlx.top",
    "icons": {
      "32": "logo.svg",
      "48": "logo.svg",
      "64": "logo.svg",
      "96": "logo.svg"
    },
    "update": {
      "versions": "https://raw.githubusercontent.com/aidenlx/obsidian-zotero/master/app/zotero/update.json",
      "download": "https://github.com/aidenlx/obsidian-zotero/releases/download/zt{version}/zotero-obsidian-note-{version}.xpi",
      "info": "https://github.com/aidenlx/obsidian-zotero/releases/tag/zt{version}"
    }
  },
  "dependencies": {
    "monkey-around": "^2.3.0",
    "assert-never": "^1.2.1",
    "@obzt/common": "workspace:*",
    "@aidenlx/zotero-helper": "workspace:*",
    "@obzt/protocol": "workspace:*",
    "@mobily/ts-belt": "^4.0.0-rc.5",
    "@obzt/database": "workspace:*"
  },
  "devDependencies": {
    "@aidenlx/eslint-config": "workspace:*",
    "@release-it/bumper": "^4.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.17.4",
    "@types/eslint": "~8.4.6",
    "zotero-types": "~1.0.4",
    "eslint": "~8.36.0",
    "typescript": "~5.0.2"
  },
  "browserslist": [
    "firefox >= 60"
  ]
}
